    "version": "0.2.0",
    "configurations": [
        {
            "name": "My TEST02",
            "type": "cppdbg",
            "request": "launch",
            "externalConsole": false,
            "program": "${workspaceFolder}/build/TEST02.exe",
            "args": [],
            "cwd": "${fileDirname}",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}